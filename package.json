{
  "name": "todomvc",
  "version": "0.0.0",
  "description": "TodoMVC example for redux",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "create_doc_with_jsdoc": "npm run create_stack_doc_with_jsdoc && npm run create_project_doc_with_jsdoc",
    "create_stack_doc_with_jsdoc": "jsdoc ./src/stack ./test/unit --recurse --destination ./doc/auto_generated/jsdoc/stackdoc --template node_modules/docdash",
    "create_project_doc_with_jsdoc": "jsdoc ./src/project ./test/unit --recurse --destination ./doc/auto_generated/jsdoc/projectdoc --template node_modules/docdash",
    "create_doc_with_esdoc_with_all_test_tags": "esdoc -c ./esdoc.conf.json",
    "create_doc_with_mocha_from_all_unit_tests": "mocha --ui bdd --compilers js:babel-register --timeout 60000 test/unit/**/*.js --reporter markdown > doc/auto_generated/mocha_markdown/doc_from_all_unit_tests.md || exit 0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaearon/redux-devtools.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "flux"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gaearon/redux-devtools/issues"
  },
  "homepage": "https://github.com/gaearon/redux-devtools#readme",
  "dependencies": {
    "classnames": "^2.1.2",
    "mathjs": "^3.10.0",
    "object-path": "^0.11.4",
    "plurals-cldr": "^1.0.3",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-hot-loader": "^3.0.0-beta.1",
    "react-redux": "^4.1.0",
    "redux": "^3.1.1",
    "round-to": "^2.0.0",
    "rrule-alt": "^2.2.5",
    "sift": "^3.2.7",
    "xspans": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.17",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015-loose": "^6.1.3",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.5.0",
    "jest": "^19.0.2",
    "node-libs-browser": "^0.5.2",
    "raw-loader": "^0.5.1",
    "react-test-renderer": "^15.4.2",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.2",
    "style-loader": "^0.12.3",
    "webpack": "^1.9.11",
    "webpack-dev-server": "^1.9.0"
  }
}
